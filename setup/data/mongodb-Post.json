{ "nodes":
	[
		{ "_id": {"$ObjectId": "50227bcd46e10eb078e56a0b"}
		, "subject": "http://magnode.org/Post"
		, "type": ["http://magnode.org/MongoDBJSONSchema"]
		, "label": "Post"
		, "description": "An authored and dated article of content, like a blog post or a magazine article."
		, "menu":
			{ "http://localhost/Menu_ManagementMenu":
				{ "title": "Posts"
				, "weight": 0
				}
			}
		, "permissions": null
		, "schema":
			{ "_schema": "http://json-schema.org/draft-03/schema#"
			, "type": "object"
			, "properties":
				{ "_id":
					{ "type": "ObjectId"
					, "readonly": true
					}
				, "_parent":
					{ "type": "ObjectId"
					, "readonly": true
					}
				, "subject":
					{ "type": "string"
					, "format": "uri"
					, "widget": "http://magnode.org/field/token"
					, "tokenPattern": "/{posted.Y}/{posted.m}/{posted.d}/{label.formatURL}"
					}
				, "type":
					{ "type": "array"
					, "widget": "array"
					, "items":
						{ "type": "string"
						, "format": "uri"
						}
					}
				, "label":
					{ "type": "string"
					, "widget": "http://magnode.org/field/Label"
					}
				, "menu":
					{ "type": "object"
					, "widget": "http://magnode.org/field/JSON"
					, "default": {}
					}
				, "body":
					{ "type": "string"
					, "widget": "http://magnode.org/field/HTML"
					}
				, "author":
					{ "type": "string"
					, "format": "uri"
					, "widget": "http://magnode.org/field/selectresource"
					, "range": "http://localhost/usergroup/Author"
					, "selectLabel": "accountName"
					, "selectNull": true
					}
				, "posted":
					{ "type": "Date"
					, "widget": "http://magnode.org/field/Date"
					, "default": "now"
					}
				}
			, "put":
				{ "storeRevision": true
				}
			}
		, "tablequery": {"fields":["_id","subject","label"]}
		, "ViewTransform":
			{ "page-example":
				{ "type": "jade"
				, "file": "template/HTMLBody_typePost.jade"
				}
			}
		}
	]
}

@base <http://localhost/> .
@prefix type: </> .
@prefix magnode: <http://magnode.org/> .
@prefix view: <http://magnode.org/view/> .
@prefix Transform: <http://magnode.org/transform/> .


# Enable page theme

<http://magnode.org/theme/twentyonetwelve/DocumentHTML_typeHTMLBody>
	a view:ViewTransform, view:FormTransform .
<http://magnode.org/theme/twentyonetwelve/HTMLBody_typePost>
	a view:ViewTransform .
<http://magnode.org/theme/twentyonetwelve/HTMLBody_typePage>
	a view:ViewTransform .


# Main menu

Transform:HTMLBody_Block_MainMenu_typeMenu_MainMenu
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.HTMLBodyBlock_typeMenu" ;
	view:domain ( type:Menu_MainMenu );
	view:range magnode:HTMLBody_Block_MainMenu .

Transform:Menu_MainMenu_typeDocument
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.Menu_typeDocument" ;
	view:menuItemContentTypes type:Raw, type:Page;
	view:domain ();
	view:range type:Menu_MainMenu .


# User menu

Transform:HTMLBody_Block_UserMenu_typeMenu_UserMenu
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.HTMLBodyBlock_typeMenu" ;
	view:domain ( type:Menu_UserMenu );
	view:range magnode:HTMLBody_Block_UserMenu .

Transform:Menu_UserMenu_typeUserSession
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.Menu_typeUserSession" ;
	view:domain ( magnode:UserSession );
	view:range type:Menu_UserMenu .


# Management menu

Transform:HTMLBody_Block_ManagementMenu_typeMenu_ManagementMenu
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.HTMLBodyBlock_typeMenu" ;
	view:domain ( type:Menu_ManagementMenu );
	view:range magnode:HTMLBody_Block_ManagementMenu .

Transform:Menu_ManagementMenu_ManagementMenu
	a view:Transform, view:FormTransform, view:ViewTransform, view:ModuleTransform ;
	view:module "magnode/transform.Menu_typeDocument" ;
	view:menuItemContentTypes magnode:MongoDBJSONSchema, type:Page;
	view:domain ();
	view:range type:Menu_ManagementMenu .



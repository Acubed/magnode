@prefix : <http://magnode.org/mongodb-manifest/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# List off all the files and the collection they get imported into
<system.indexes/nodes.subject.json> a :MongoDocument; :collection "system.indexes" .
<schema/Schema.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<../jsonschema/schema.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<../jsonschema/hyperschema.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/AuthCallback.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/AuthClient.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/AuthEndpoint.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Namespace.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/DocumentRegion.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Entry.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/LinkMenu.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/LinkMenuItem.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/List.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/OnlineAccount.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Page.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Post.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Term.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<schema/Usergroup.json> a :MongoDocument; :collection "schema" ; :mediaType "application/json;profile=http://magnode.org/MongoDBJSONSchema" .
<documentregion/footer.json> a :MongoDocument; :collection "documentregion" .
<documentregion/header.json> a :MongoDocument; :collection "documentregion" .
<documentregion/panel.json> a :MongoDocument; :collection "documentregion" .
<group/administrator.json> a :MongoDocument; :collection "group" .
<group/author.json> a :MongoDocument; :collection "group" .
<group/user.json> a :MongoDocument; :collection "group" .
<linkmenu/main.json> a :MongoDocument; :collection "linkmenu" .
<linkmenu/management.json> a :MongoDocument; :collection "linkmenu" .
<linkmenu/settings.json> a :MongoDocument; :collection "linkmenu" .
<nodes/frontpage.json> a :MongoDocument; :collection "list" .
<nodes/intro.json> a :MongoDocument; :collection "nodes" .
<taxonomy/Taxonomy.json> a :MongoDocument; :mediaType "application/json;profile=http://magnode.org/Term" .
<taxonomy/PublishOption.json> a :MongoDocument; :mediaType "application/json;profile=http://magnode.org/Term" .
<taxonomy/PublishOptionLocal.json> a :MongoDocument; :mediaType "application/json;profile=http://magnode.org/Term" .
<taxonomy/promoted.json> a :MongoDocument; :mediaType "application/json;profile=http://magnode.org/Term" .
<taxonomy/published.json> a :MongoDocument; :mediaType "application/json;profile=http://magnode.org/Term" .

# List off groups of files that should be imported together
<#bundles> :bundleCollection (
	[
		# This needs to be inserted before anything else
		# It bootstraps everything necessary to get content indexing working
		a :Feature;
		rdfs:label "Base";
		:required true;
		:default true;
		:files ( <schema/Schema.json> <system.indexes/nodes.subject.json> <../jsonschema/schema.json> <../jsonschema/hyperschema.json> <schema/Namespace.json> )
	]
	[
		a :Feature;
		rdfs:label "User Accounts";
		:required true;
		:default true;
		:files ( <schema/OnlineAccount.json> <schema/Usergroup.json> <schema/AuthEndpoint.json> <schema/AuthClient.json> <schema/AuthCallback.json> )
	]
	[
		a :Feature;
		rdfs:label "Basic Content Types";
		:required false;
		:default true;
		:files ( <schema/List.json> <schema/Entry.json> <schema/Page.json> <schema/Post.json> )
	]
	[
		a :Feature;
		rdfs:label "Sample Data";
		:required false;
		:default true;
		:files ( <nodes/frontpage.json> <nodes/intro.json> )
	]
	[
		a :Feature;
		rdfs:label "Theme Data";
		:required false;
		:default true;
		:files ( <schema/DocumentRegion.json> <schema/LinkMenu.json> <schema/LinkMenuItem.json> <documentregion/footer.json> <documentregion/header.json> <documentregion/panel.json> <linkmenu/main.json> <linkmenu/management.json> <linkmenu/settings.json> )
	]
	[
		a :Feature;
		rdfs:label "Taxonomy";
		:required false;
		:default true;
		:files ( <schema/Term.json> <taxonomy/Taxonomy.json> )
	]
	[
		a :Feature;
		rdfs:label "Publish Options";
		:required false;
		:default true;
		:files ( <taxonomy/PublishOption.json> <taxonomy/promoted.json> <taxonomy/published.json> )
	]
	[
		a :Feature;
		rdfs:label "Sample UserGroups";
		:required false;
		:default true;
		:files ( <group/administrator.json> <group/author.json> <group/user.json> )
	]
) .


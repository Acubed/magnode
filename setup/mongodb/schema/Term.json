, "type": "object"
, "links":
	[ {"rel":"type", "href":"http://magnode.org/Term"}
	, {"rel":"self", "href":"{+subject}"}
	]
, "properties":
	{ "_id": { "type": "ObjectId", "readonly": true }
	, "subject": { "type": "string", "format": "uri" }

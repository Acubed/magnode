[submodule "node_modules/rdf"]
	path = node_modules/rdf
	url = git://github.com/Acubed/node-rdf.git
	semver = *
	npm = rdf
[submodule "node_modules/sparql-spin.src"]
	path = node_modules/sparql-spin.src
	url = git://github.com/Acubed/sparql-spin-js3.git
	semver = *
[submodule "node_modules/jsonschema"]
	path = node_modules/jsonschema
	url = git://github.com/Acubed/jsonschema.git
	semver = *
	npm = jsonschema
